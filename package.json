{
  "name": "prompt-canvas",
  "version": "0.0.5",
  "private": false,
  "description": "Visual prompts for coding agents directly from your frontend",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adiasg/prompt-canvas.git"
  },
  "bugs": {
    "url": "https://github.com/adiasg/prompt-canvas/issues"
  },
  "homepage": "https://github.com/adiasg/prompt-canvas#readme",
  "type": "module",
  "sideEffects": false,
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./client.js",
      "require": "./dist/index.cjs",
      "default": "./client.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "client.js"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "dependencies": {
    "@radix-ui/react-tooltip": "^1.1.2",
    "html-to-image": "^1.11.11",
    "lucide-react": "^0.460.0"
  },
  "devDependencies": {
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "rimraf": "^5.0.7",
    "tsup": "^8",
    "typescript": "^5"
  },
  "publishConfig": {
    "access": "public"
  }
}
